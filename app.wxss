page {
  line-height: 1;
  font-size: 14px;
  color: #333333;
  font-weight: 200;
	font-family: 'PingFang SC','STHeitiSC-Light','Helvetica-Light',arial, sans-serif;
}
.hide {
  display: none
}
.block {
  display: block
}
.over_1 {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
}

.over_2 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden
}
.clearfix:after {
	content: '';
	display: block;
	width: 0;
	height: 0;
	overflow: hidden;
	clear: both
}
.fl {
	float: left
}

.fr {
	float: right
}

.tl {
	text-align: left
}

.tc {
	text-align: center
}

.tr {
	text-align: right
}
.btn {
	display: block;
	line-height: 42px;
	color: #fff;
	width: 100%;
	text-align: center;
	font-size: 16px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px
}
.btn-primary {
	background-color: #724d93;
}
.fv, .fh, .fhv, .fs {display: flex; display: -webkit-flex;}
.fv {align-items: center; -webkit-align-items: center;}/*垂直居中*/
.fh {justify-content: center; -webkit-justify-content: center;}/*水平居中*/
.fs {justify-content: space-between; -webkit-justify-content: space-between;}
.fhv {justify-content: center; -webkit-justify-content: center;align-items: center; -webkit-align-items: center;}/* 垂直水平居中 */
.mask {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
}
.vh {
  visibility: hidden
}
/* 空页面S */
.empty-wrap {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
	padding: 40px 12px;
  background: #fff;
	text-align: center;
}
.empty-wrap .img {
	display: block;
	width: 30%;
	margin: 30% auto 0;
}
.empty-wrap .text {
	color: #999999;
	font-size: 14px;
	margin: 15px 0;
}
.empty-wrap .link {
  display: inline;
  color: #ff9519;
}
/* 空页面 */
/* 模态框 S */
.modal-container {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  color: #333;
  text-align: center;
  z-index: 99
}
.modal-container .mask-content {
  position: absolute;
  width: 562rpx;
  top: 22%;
  left: 50%;
  border-radius: 20px;
  margin-left: -281rpx;
  background: #fff;
  font-size: 16px;
}
.modal-container .mask-content .title {
  margin: 24px 0;
}
.modal-container .mode-select .item {
  padding-bottom: 24px;
  line-height: 19px;
}
.mode-select .item .icon {
  position: relative;
  display: inline-block;
  vertical-align: top;
  width: 17px;
  height: 17px;
  color: #d9d8d8;
  border: 1px solid;
  border-radius: 50%;
  margin-right: 12px;
}
.mode-select .item .icon.selected {
  color: #724d93;
}
.mode-select .item .icon.selected::after {
  background: #724d93;
}
.mode-select .item .icon::after {
  content: '';
  position: absolute;
  left: 4px;
  right: 4px;
  bottom: 4px;
  top: 4px;
  border-radius: 50%;
  background-color: #d9d8d8;
}
.mask-content .close {
  position: absolute;
  width: 20px;
  height: 20px;
  top: -25px;
  right: 0;
}
.mask-content .slide-wrap .item {
  display: flex;
  display: -webkit-flex;
  padding: 0 15px;
  margin: 27px 0;
}
.mask-content.standard {
  padding-bottom: 10px;
}
.mask-content .slide-wrap .item .text {
  width: 160rpx;
  font-size: 14px;
  text-align: left;
}
.mask-content .slide-wrap .item .slide {
  position: relative;
  flex: 1;
  -webkit-flex: 1;
  border-radius: 20px;
  border: 8rpx solid #e9e9e9;
}
.mask-content .slide-wrap .item .slide .f {
  height: 20rpx;
  background-color: #bdbdbd;
  border-radius: 20px;
}
.mask-content .slide-wrap .item .slide .value {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  border-radius: 20px;
}
.mask-content .slide-wrap .item .slide .drag {
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  left: -1px;
  top:50%;
  margin-top:-28rpx;
  padding:10rpx;
  margin-left: -15rpx;
}
.mask-content.bettery .text {
  padding: 0 76rpx;
  line-height: 24px;
  font-size: 16px;
}
.mask-content.bettery .button {
  margin-top: 15px;
  line-height: 45px;
  border-top: 1px solid #e8e8e8;
  color: #724d93;
  font-size: 16px;
}
/* 模态框 E */
/* 下拉刷新 S */
.pull-refresh {
  width: 100%;
  padding: 10px 0;
  line-height: 20px;
  font-size: 14px;
  z-index: 11;
  text-align: center;
}
.pull-refresh .icon {
  width: 20px;
  height: 20px;
  vertical-align: top;
  margin-right: 8px;
}
.rotate {
  animation: rotate 1s linear;
  -webkit-animation: rotate 1s linear;
  animation-iteration-count: infinite;
  -webkit-animation-iteration-count: infinite;
}
@keyframes rotate {
  from {
    transform: rotateZ(0deg);
    -webkit-transform: rotateZ(0deg);
  }
  to {
    transform: rotateZ(360deg);
    -webkit-transform: rotateZ(360deg);
  }
}
/* 下拉刷新 S */